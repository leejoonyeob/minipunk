{
  "containerDefinitions": [
    {
      "name": "cicdContainer",
      "image": "<IMAGE1_NAME>",
      "portMappings": [
        {
          "containerPort": 5000,
          "hostPort": 5000,
          "protocol": "tcp",
          "name": "gameport",
          "appProtocol": "http"
        }
      ],
      "essential": true
    }
  ],
  "family": "cicd",
  "executionRoleArn": "arn:aws:iam::463470950104:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "1024",
  "memory": "2048"
}
